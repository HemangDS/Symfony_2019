                                        <div class="cash-bck">
                                            {%if category.shop_offers == 'cashback'%}
                                                {% if category.slug_name is defined %}
                                                    <a href ="{{absolute_url(path('marcas_view', {'slug_name': category.slug_name})) }}">
                                                        <span>+
                                                            {%if category.shop_offers == 'voucher'%}
                                                                {{ category.discount_amount }}
