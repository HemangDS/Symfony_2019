                                            {% if category.voucher_code_count is defined%}
                                                {% if category.voucher_code_count is not empty%}
                                                    {% if category.slug_name is defined %}
                                                        <a href ="{{absolute_url(path('marcas_view', {'slug_name': category.slug_name})) }}">
