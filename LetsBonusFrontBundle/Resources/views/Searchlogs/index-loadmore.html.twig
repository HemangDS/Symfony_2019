                                                                <h6>Cashback en esta tienda:</h6>
                                                            </div>
                                                            <div class="cup-text">
                                                                {% for variation in category.shop_history_variation %}
                                                                    {% if "%" in variation.number or "€" in variation.number %}
                                                                    <p><span>{{variation.number}}</span>{{variation.title}}</p>
                                                                    {% else %}
                                                                    <p><span>{{variation.number}}%</span>{{variation.title}}</p>
                                                                    {% endif %}
                                                                {% endfor %}
                                                                {#
                                                                    <p><span>6%</span>Bolsos, cuidado personal, libros</p>
                                                                    <p><span>1,88%</span>CDs, videojuegos y DvDs</p>
                                                                    <p><span>0,75%</span>Tecnología y electrodomésticos de cuidado personal</p>
                                                                #}
                                                            </div>
                                                        {% endif %}
                                                    {% endif %}
                                                    <div class="cupon-list-box">
                                                        {% if category.shop_terms is defined%}
                                                            {% if category.shop_terms is not empty%}
                                                                <h6>Condiciones:</h6>
                                                                <ul>
                                                                    <li>
                                                                        {% autoescape %}
                                                                            {{ category.shop_terms|raw  }}
                                                                        {% endautoescape %}
                                                                    </li>
                                                                </ul>
                                                            {% endif %}
                                                        {% endif %}
                                                    </div>
                                                {% endif %}
                                            </div>
                                        </div>
                                        <div class="image-btn-suer-coupon">
                                            {%if category.shop_offers == 'offer'%}
                                              {%if category.voucher_code is defined %}
                                                <a href="javascript:void(0);" title="Usar cupón" onclick="offerGenerate('{{category.shop_id}}','{{category.shop_history_id}}','{{category.shop_offers}}','{{ tabType }}','{{ tabId }}','{{ i }}','{{absolute_url(path('i_flair_lets_bonus_front_clicks')) }}','{{category.shop_affiliate_url}}','{{user_id}}',event, '{{ category.brand_logo_popup }}', '{{ category.voucher_code }}', '{{ category.default_track_uri }}', '{{ category.voucher_program_name }}', '{{ category.discount_amount }}', '{{ category.is_percentage }}', '{{ category.voucher_expire_date }}');">
                                                        Ver oferta
                                                </a>
                                              {% endif %}                                                    
                                             {%endif%}
                                            {%if category.shop_offers == 'voucher'%}
                                                {%if category.voucher_code is defined %}
                                                    {%if category.voucher_code is not empty %}
                                                        <a href="javascript:void(0);" title="Usar cupón" onclick="cuponGenerate('{{category.shop_id}}','{{category.shop_history_id}}','{{category.shop_offers}}','{{ tabType }}','{{ tabId }}','{{ i }}','{{absolute_url(path('i_flair_lets_bonus_front_clicks')) }}','{{category.shop_affiliate_url}}','{{user_id}}',event, '{{ category.brand_logo_popup }}', '{{ category.voucher_code }}', '{{ category.default_track_uri }}', '{{ category.voucher_program_name }}', '{{ category.discount_amount }}', '{{ category.is_percentage }}', '{{ category.voucher_expire_date }}');" class="coupon-tail">
                                                            Usar Cupón
                                                        </a>
                                                    {% else %}
                                                        <a href="javascript:void(0);" title="Usar cupón" onclick="offerGenerate('{{category.shop_id}}','{{category.shop_history_id}}','{{category.shop_offers}}','{{ tabType }}','{{ tabId }}','{{ i }}','{{absolute_url(path('i_flair_lets_bonus_front_clicks')) }}','{{category.shop_affiliate_url}}','{{user_id}}',event, '{{ category.brand_logo_popup }}', '{{ category.voucher_code }}', '{{ category.default_track_uri }}', '{{ category.voucher_program_name }}', '{{ category.discount_amount }}', '{{ category.is_percentage }}', '{{ category.voucher_expire_date }}');">
                                                            Ver oferta
                                                        </a>
                                                    {% endif %}
                                                {% endif %}
                                            {%endif%}
                                            {%if category.shop_offers == 'cashback'%}
                                                {% if category.cashbackPrice is not empty and category.cashbackPrice != '0' %}
                                                    <a href="javascript:void(0);" title="Usar cupón" onclick="registerClick('{{category.shop_id}}','{{category.shop_history_id}}','{{category.shop_offers}}','{{ tabType }}','{{ tabId }}','{{ i }}','{{absolute_url(path('i_flair_lets_bonus_front_clicks')) }}','{{category.shop_affiliate_url}}','{{user_id}}',event, '{{ category.brand_logo_popup }}', '{{ category.cashbackPrice }}€', '{{category.shop_affiliate_url_origin}}', '{{category.program_id}}');" target="_blank">
                                                        Conseguir cashback
                                                    </a>
                                                {% else %}
                                                    <a href="javascript:void(0);" title="Usar cupón" onclick="registerClick('{{category.shop_id}}','{{category.shop_history_id}}','{{category.shop_offers}}','{{ tabType }}','{{ tabId }}','{{ i }}','{{absolute_url(path('i_flair_lets_bonus_front_clicks')) }}','{{category.shop_affiliate_url}}','{{user_id}}',event, '{{ category.brand_logo_popup }}', '{{ category.cashbackPercentage }}%', '{{category.shop_affiliate_url_origin}}', '{{category.program_id}}');" target="_blank">
                                                        Conseguir cashback
                                                    </a>
                                                {% endif %}
                                            {%endif%}
                                        </div>
                                    </div>
                                </div>
                                {#
                                    End class category-blog-main-right-middle
                                #}
