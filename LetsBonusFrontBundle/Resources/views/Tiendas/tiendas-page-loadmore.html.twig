              {% endfor %}
              <span><a href="javascript:void(0);" {% if alphabet == '0TO9'%} class= "active" {%endif%}>0-9</a></span>
              <span><a href="javascript:void(0);" 
              {% if alphabet == 'TODAS'%} class= "active" {%endif%} 
                {% if alphabet is null %} class= "active" {%endif%}
              >TODAS</a></span>
